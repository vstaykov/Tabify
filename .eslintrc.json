{
    "root": true,
    "extends": [
      "airbnb"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "chrome": false
    },
    "rules": {
        "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
    }
  }