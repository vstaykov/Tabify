{
  "manifest_version": 2,
  "name": "Tabify",
  "version": "1.0",
  "description": "Manages different aspects of your tabs interactions",
  "icons": {
    "16": "icons/tabs_16.png",
    "24": "icons/tabs_24.png",
    "32": "icons/tabs_32.png",
    "64": "icons/tabs_64.png",
    "128": "icons/tabs_128.png"
  },
  "minimum_chrome_version": "23",
  "browser_action": {
    "default_icon": "icons/tabs_512.png",
    "default_popup": "public/html/popup.html"
  },
  "background": {
    "scripts": [
      "dist/backgroundscript.bundle.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "public/html/options.html",
    "open_in_tab": true
  },
  "commands": "CREATED BUILD TIME!",
  "permissions": [
    "tabs",
    "commands",
    "storage"
  ]
}
