{
  "name": "tabify",
  "version": "1.0.0",
  "description": "Google Chrome extension for managing tabs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vstaykov/tabify.git"
  },
  "author": "Ventsislav Staykov",
  "homepage": "https://github.com/vstaykov/tabify#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "scripts": {
    "build": "npm run build:eventPage & npm run build:popup",
    "build:eventPage": "browserify src/event-page.js src/commands-executor.js -o dist/event-page.js -t babelify",
    "build:popup": "browserify src/popup.js -o dist/popup.js -t babelify"
  }
}
