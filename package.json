{
  "name": "tabify",
  "version": "1.0.0",
  "description": "Google Chrome extension for managing tabs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vstaykov/tabify.git"
  },
  "author": "Ventsislav Staykov",
  "homepage": "https://github.com/vstaykov/tabify#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "scripts": {
    "build:win": "npm run build:js & npm run build:content:win",
    "build:lin": "npm run build:js & npm run build:content:lin",
    "build:js": "npm run build:eventPage & npm run build:popup",
    "build:eventPage": "browserify src/eventPage.js src/CommandsExecutor.js -o dist/eventPage.js -t babelify",
    "build:popup": "browserify src/popup.js -o dist/popup.js -t babelify",
    "build:content:win": "robocopy node_modules/bootstrap/dist/css/bootstrap.min.css public/styles/bootstrap.min.css",
    "build:content:lin": "cp -R node_modules/bootstrap/dist/css/bootstrap.min.css public/styles/bootstrap.min.css"
  }
}
