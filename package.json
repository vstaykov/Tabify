{
  "name": "tabify",
  "version": "1.0.0",
  "description": "Google Chrome extension for managing tabs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vstaykov/tabify.git"
  },
  "author": "Ventsislav Staykov",
  "homepage": "https://github.com/vstaykov/tabify#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0"
  },
  "dependencies": {},
  "scripts": {
    "build:win": "npm run build:js & npm run build:content:win",
    "build:lin": "npm run build:js & npm run build:content:lin",
    "build:js": "browserify src/eventPage.js src/CommandsExecutor.js -o dist/eventPage.js -t babelify",
    "build:content:win": "robocopy src/html dist/html",
    "build:content:lin": "cp -R src/html dist"
  }
}
